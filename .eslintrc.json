{
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "import/extensions": ["error", "always", { "ignorePackages": true, "jsx": "always" }],
    "no-unused-vars":"warn",
    "react/prop-types": ["off"]
  }
}
